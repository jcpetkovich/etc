# ${_colors_} defined in env

# ......................................................... Distribution install

# clean up environment for possible re-install of package
if [ -s $HOME/build/archlinux/pre_install/$1 ] ;then
  arch && echo "${_GREEN_}@ pre_install/$1${_nocolor_}"
  $HOME/build/archlinux/pre_install/$1
fi

